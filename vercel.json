{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "docs/.vitepress/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/docs/.vitepress/dist/$1"
        }
    ]
}